{
  "name": "elementor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:public-site": "ng serve public-site --port 4201",
    "start:admin": "ng serve admin --port 4200",
    "start:all": "npm run start:admin & npm run start:public-site",
    "build": "ng build",
    "build:public-site": "ng build public-site",
    "build:admin": "ng build admin",
    "build:all": "npm run build:admin & npm run build:public-site",
    "watch": "ng build --watch --configuration development",
    "watch:public-site": "ng build --watch --configuration development --project public-site",
    "watch:admin": "ng build --watch --configuration development --project admin",
    "watch:all": "npm run watch:admin & npm run watch:public-site",
    "test": "ng test",
    "serve:ssr:page-builder": "node dist/page-builder/server/server.mjs",
    "serve:ssr:public-site": "node dist/public-site/server/server.mjs",
    "serve:ssr:builder-admin": "node dist/builder-admin/server/server.mjs",
    "serve:ssr:admin": "node dist/admin/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.22.0",
  "dependencies": {
    "@angular/aria": "21.0.1",
    "@angular/cdk": "^21.0.1",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "21.0.1",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/ssr": "^21.0.0",
    "express": "^5.1.0",
    "material-symbols": "^0.40.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "ng-packagr": "^21.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2"
  }
}